.wrapper {
  width: calc(100% - 9rem);
  @apply fixed left-36 transition-all top-0 z-20;
}

.header {
  @apply h-20 flex items-center dark:bg-gray-900 bg-white justify-between px-10 w-full;

  .rightBlock {
    @apply flex items-center;
    .rightItem {
      @apply ml-5;
    }
  }

  nav {
    a {
      @apply mr-20 uppercase font-extrabold;
    }
  }
}
