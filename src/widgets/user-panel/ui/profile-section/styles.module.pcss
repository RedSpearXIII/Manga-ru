.profileSection {
  @apply p-2 flex items-center justify-between w-full h-full;

  .left {
  }
  .right {
    @apply flex h-full items-center;

    .separator {
      @apply h-full w-[1px] mr-3 dark:bg-slate-600 bg-slate-300;
    }
    .logoutBtn {
      @apply flex flex-col items-center justify-center text-red-500 dark:text-red-400 cursor-pointer;
      svg {
        @apply text-2xl;
      }
      p {
        @apply text-sm;
      }
    }
  }
}
