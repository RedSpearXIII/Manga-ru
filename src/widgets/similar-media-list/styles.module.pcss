.list {
  @apply grid max-md:grid-cols-1 max-[1110px]:grid-cols-2 grid-cols-[repeat(auto-fill,285px)] gap-3 mt-2 justify-between;
}
.listShowToggler {
  @apply dark:hover:text-orange-300 hover:text-orange-400 cursor-pointer transition-colors;
}

.listClosed {
  @apply max-h-[280px] overflow-hidden;
}
