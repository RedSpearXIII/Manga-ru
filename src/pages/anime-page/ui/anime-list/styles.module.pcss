.list {
  @apply grid grid-cols-[repeat(auto-fill,185px)] mx-auto justify-between gap-y-6 gap-x-8;
}

.error {
  @apply flex items-center justify-center;

  svg {
    @apply mr-5 text-2xl text-red-500;
  }
}

.notFound {
  @apply flex items-center justify-center;

  svg {
    @apply mr-5 text-2xl;
  }
}
